<% all_categories.each do |category| %>
  <%= render :partial => 'categories/category', :locals => { :category => category } %>
<% end %>

<script>
  $(document).ready(function(){
    //Categories do not have pagination. No update pageless.
    if(typeof re_apply_backstrech == "function"){
      re_apply_backstrech("categories");
    }
  });
</script>